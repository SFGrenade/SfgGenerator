  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug clap-validator",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "C:\\_Programs\\clap-validator\\clap-validator.exe",
      "args": [
        "validate",
        "--in-process",
        "--test-filter",
        "state-reproducibility-basic",
        "${workspaceFolder}\\build\\windows\\x64\\debug\\SfgGenerator.clap"
